<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.patientSearchController">
   <children>
      <AnchorPane prefHeight="386.0" prefWidth="640.0">
         <children>
            <Label layoutX="128.0" layoutY="66.0" prefHeight="27.0" prefWidth="114.0" text="First name">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="128.0" layoutY="119.0" prefHeight="17.0" prefWidth="101.0" text="Last Name ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="128.0" layoutY="188.0" prefHeight="59.0" prefWidth="150.0" text="Birthdate ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutX="16.0" layoutY="322.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="39.0" prefWidth="124.0" text="Back">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="searchButton" layoutX="493.0" layoutY="322.0" mnemonicParsing="false" onAction="#handleSearch" prefHeight="39.0" prefWidth="124.0" text="Search">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <TextField fx:id="firstNameBox" layoutX="246.0" layoutY="67.0" promptText="Patient's First Name" />
            <TextField fx:id="lastNameBox" layoutX="246.0" layoutY="120.0" promptText="Patient's First Name" />
            <TextField fx:id="birthdateBox" layoutX="242.0" layoutY="205.0" promptText="Patient's Birthdate" />
         </children>
         <effect>
            <Blend mode="DARKEN" />
         </effect>
      </AnchorPane>
   </children>
</VBox>
